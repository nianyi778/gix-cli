{
  "name": "gix-cli",
  "version": "1.2.0",
  "description": "A Git extension CLI tool",
  "bin": {
    "gix": "dist/gix.js"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node --loader ts-node/esm src/bin/gix.ts",
    "build": "tsup",
    "prepare": "pnpm build"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/inquirer": "^9.0.7",
    "ts-node": "^10.9.1",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "git",
    "cli",
    "commit",
    "merge",
    "squash",
    "git tools",
    "git merge",
    "git extension",
    "command-line",
    "developer tools",
    "typescript"
  ],
  "packageManager": "pnpm@10.7.1"
}
